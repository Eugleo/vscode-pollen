{
    "name": "Pollen HTML preprocessor",
    "scopeName": "text.html.pollen.preprocessor",
    "firstLineMatch": "^#lang\\s+pollen\\s*$",
    "filetypes": [
        "html.pp"
    ],
    "injections": {
        "meta.function-call.racket": {
            "patterns": [
                {
                    "include": "source.pollen.preprocessor"
                }
            ]
        }
    },
    "patterns": [
        {
            "include": "text.html.basic"
        },
        {
            "include": "source.pollen.preprocessor"
        }
    ]
}