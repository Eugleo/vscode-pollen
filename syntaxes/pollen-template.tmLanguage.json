{
    "name": "Pollen Template",
    "scopeName": "source.pollen.template",
    "filetypes": [
        "p"
    ],
    "injections": {
        "L:meta.function-call.racket": {
            "patterns": [
                {
                    "name": "support.constant.doc.pollen",
                    "match": "\\bdoc\\b"
                },
                {
                    "name": "support.constant.here.pollen",
                    "match": "\\bhere\\b"
                },
                {
                    "include": "source.pollen.preprocessor"
                }
            ]
        }
    },
    "patterns": [
        {
            "include": "source.pollen.preprocessor"
        }
    ]
}