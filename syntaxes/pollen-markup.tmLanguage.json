        "highlight-tags": {
            "patterns": [
                { "include": "#embedded-code-highlight-tag" },
                { "include": "#embedded-code-custom-tag" }
            ]
        },
        "function-body-parameter": {
            "name": "meta.function.body.pollen",
            "begin": "\\{",
            "beginCaptures": {
                "0": { "name": "punctuation.definition.body.pollen" }
            },
            "end": "\\}",
            "endCaptures": {
                "0": { "name": "punctuation.definition.body.pollen" }
            },
            "patterns": [
                { "include": "$self" },
                { "include": "#braces" }
            ]
        },
        "function-body-parameter/racket": {
            "name": "meta.function.body.pollen",
