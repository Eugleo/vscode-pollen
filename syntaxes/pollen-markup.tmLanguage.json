                    "endCaptures": {
                        "0": { "name": "punctuation.definition.function.pollen" }
                    },
                    "patterns": [
                        { "include": "#function-parameters" },
                        { "include": "#function-body-parameter/css" }
                    ]
                },
                {
                    "name": "meta.function.expression.pollen",
                    "begin": "(◊)([^\\s\\{\\}\\(\\)\\[\\]#\\|,\"'´\\\\;]*highlight[^\\s\\{\\}\\(\\)\\[\\]#\\|,\"'´\\\\;]*)(?=\\[(.*\\s+|\\s*)'(js|javascript)\\b.*\\]\\{)",
                    "beginCaptures": {
                        "1": { "name": "punctuation.definition.function.pollen"},
                        "2": { "name": "entity.name.function.pollen" }
