{
    "name": "Pollen markup",
    "scopeName": "text.pollen",
    "filetypes": [
        "pm"
    ],
    "firstLineMatch": "^#lang\\s+pollen",
    "patterns": [
        { "include": "#highlight-tags" },
        { "include": "#latex-math" },
        { "include": "source.pollen.preprocessor" }
        
    ],
    "repository": {
        "latex-math": {
            "contentName": "meta.embedded.latex",
            "begin": "(◊\\${1,2})(\\{)",
            "beginCaptures": {
                "1": { "name": "punctuation.definition.function.pollen" },
                "2": { "name": "punctuation.definition.body.pollen" }
            },
            "end": "\\}",
            "endCaptures": {
                "0": { "name": "punctuation.definition.body.pollen" }
            },
            "patterns": [
                { "include": "#latex-braces" },
                { "include": "text.tex.latex" }
            ]
        },
        "embedded-code-custom-tag": {
            "patterns": [
                {
                    "name": "meta.function.expression.pollen",
                    "begin": "(◊)([^\\s\\{\\}\\(\\)\\[\\]#\\|,\"'´\\\\;]*highlight[^\\s\\{\\}\\(\\)\\[\\]#\\|,\"'´\\\\;]*)(?=\\[.*\\s+'racket\\b.*\\]\\{)",
